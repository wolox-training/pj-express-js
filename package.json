{
  "name": "WTraining",
  "version": "0.1.0",
  "description": "WTraining",
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "console": "node console.js",
    "cover": "npm run test --coverage",
    "test": "NODE_ENV=testing jest --runInBand --forceExit --detectOpenHandles",
    "test-inspect": "NODE_ENV=testing node --inspect --debug-brk jest",
    "eslint-check": "eslint --print-config .eslintrc.js --ignore-pattern ./.eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint \"**/*.js\" --ignore-pattern ./.eslintrc.js",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.js$ | xargs eslint",
    "lint-fix": "eslint \"**/*.js\" --fix --ignore-pattern ./.eslintrc.js",
    "precommit": "npm run lint-diff",
    "outdated": "npm outdated --depth 0",
    "pretest": "npm run lint",
    "start-dev": "nodemon --inspect server.js",
    "prestart-dev": "npm run lint",
    "migrations": "sequelize db:migrate",
    "start": "node server.js"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "app.js",
  "author": "Wolox",
  "homepage": "https://github.com/wolox-training/pj-express-js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wolox-training/pj-express-js.git"
  },
  "bugs": {
    "url": "https://github.com/wolox-training/pj-express-js/issues",
    "email": "tls@wolox.com.ar"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "**/*.js",
      "!**/console.js",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/migrations/**",
      "!**/config/**",
      "!**/scripts/**"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "express": "^4.17.1",
    "express-wolox-logger": "1.1.0",
    "factory-girl": "^5.0.4",
    "jwt-simple": "^0.5.1",
    "pg": "^7.4.1",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "sequelize": "^5.10.1",
    "swagger-ui-express": "^4.0.7",
    "umzug": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^25.0.0",
    "babel-preset-es2015": "6.24.1",
    "dotenv": "^5.0.1",
    "eslint": "^5.9.0",
    "eslint-config-wolox": "^3.0.2",
    "eslint-config-wolox-node": "^2.2.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^0.14.3",
    "istanbul": "^0.4.3",
    "jest": "^24.0.0",
    "mocha": "^5.0.1",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "prompt": "^1.0.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "^3.4.2"
  }
}
